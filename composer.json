{
  "name": "usabilitydynamics/wp-cluster",
  "description": "WP-Cluster.",
  "type": "wordpress-plugin",
  "version": "0.4.2",
  "homepage": "https://github.com/usabilitydynamics/wp-cluster",
  "keywords": [
    "wordpress",
    "multisite",
    "varnish"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andy Potanin",
      "homepage": "http://usabilitydynamics.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "config": {
    "preferred-install": "source",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "component-dir": "vendor/components",
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=5.3",
    "composer/installers": "~1.0",
    "usabilitydynamics/lib-installer": "master",
    "usabilitydynamics/lib-settings": "master",
    "usabilitydynamics/lib-ui": "master",
    "usabilitydynamics/lib-utility": "0.3.2",
    "usabilitydynamics/lib-api": "master",
    "usabilitydynamics/lib-model": "master",
    "usabilitydynamics/lib-saas": "master",
    "usabilitydynamics/lib-cdn": "master",
    "usabilitydynamics/lib-requires": "master",
    "usabilitydynamics/lib-rpc": "master"
  },
  "autoload": {
    "classmap": [
      "lib/class-api.php",
      "lib/class-bootstrap.php",
      "lib/class-dashboard.php",
      "lib/class-database.php",
      "lib/class-developer.php",
      "lib/class-locale.php",
      "lib/class-plugins.php",
      "lib/class-settings.php",
      "lib/class-sunrise.php",
      "lib/class-theme.php",
      "lib/class-utility.php",
      "lib/class-views.php"
    ]
  },
  "dist": {
    "url": "https://github.com/usabilitydynamics/wp-cluster/archive/master.zip",
    "type": "zip"
  },
  "source": {
    "url": "git@github.com:UsabilityDynamics/wp-cluster",
    "type": "git",
    "reference": "master"
  }
}

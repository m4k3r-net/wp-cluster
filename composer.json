{
  "name": "usabilitydynamics/wp-cluster",
  "type": "library",
  "description": "Veneer network",
  "version": "0.4.2",
  "homepage": "https://github.com/usabilitydynamics/wp-cluster",
  "keywords": [
    "wordpress",
    "multisite",
    "varnish"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andy Potanin",
      "homepage": "http://usabilitydynamics.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "require": {
    "usabilitydynamics/lib-api": "*",
    "usabilitydynamics/lib-models": "*",
    "usabilitydynamics/lib-settings": "*",
    "usabilitydynamics/lib-saas": "*",
    "usabilitydynamics/lib-ui": "*",
    "usabilitydynamics/lib-utility": "*",
    "usabilitydynamics/lib-cdn": "*",
    "usabilitydynamics/lib-asset": "*",
    "usabilitydynamics/lib-wp-xmlrpc": "*"
  },
  "autoload": {
    "classmap": [
      "lib/class-api.php",
      "lib/class-bootstrap.php",
      "lib/class-cache.php",
      "lib/class-dashboard.php",
      "lib/class-debug.php",
      "lib/class-developer.php",
      "lib/class-image.php",
      "lib/class-locale.php",
      "lib/class-log.php",
      "lib/class-media.php",
      "lib/class-mapping.php",
      "lib/class-security.php",
      "lib/class-settings.php",
      "lib/class-theme.php",
      "lib/class-utility.php",
      "lib/class-varnish.php",
      "lib/class-views.php"
    ]
  }
}
